<div data-controller="hello">
  <input data-hello-target="name" type="text" placeholder="stimulus-test">
  <button data-action="click->hello#greet">Greet</button>
</div>

<%= form_with url: registrations_path,
  data: {
     controller: "new-registration", 
     action: "ajax:success->new-registration#create ajax:error->new-registration#error" 
     } do |form| %>
  <%= form.text_field :username, placeholder: 'username' %>
  <%= form.submit 'Sign Up' %>
<% end %>
